{
  "name": "rollodeqc-gh-user-streak",
  "version": "3.0.0",
  "description": "Fetch github user commit streak.",
  "license": "AGPL-3.0",
  "repository": "millette/rollodeqc-gh-user-streak",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier-standard",
      "git add"
    ]
  },
  "scripts": {
    "lint": "standard",
    "format": "prettier-standard '**/*.+(js|json|md)'",
    "test": "standard && nyc ava --no-power-assert",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "rollodeqc"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "got": "^9.6.0",
    "lodash.flatten": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "meow": "^5.0.0",
    "update-notifier": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "nyc": "^14.1.0",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1"
  }
}
