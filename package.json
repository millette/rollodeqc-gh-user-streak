{
  "name": "rollodeqc-gh-user-streak",
  "version": "2.2.0",
  "description": "Fetch github user commit streak.",
  "license": "AGPL-3.0",
  "repository": "millette/rollodeqc-gh-user-streak",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier-standard", "git add"]
  },
  "scripts": {
    "lint": "standard",
    "format": "prettier-standard '**/*.+(js|json|md)'",
    "test": "standard && nyc ava --no-power-assert",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": ["index.js", "cli.js"],
  "keywords": ["cli-app", "cli", "rollodeqc"],
  "dependencies": {
    "chalk": "^2.4.1",
    "got": "^9.2.2",
    "lodash.flatten": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.2",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "nyc": "^13.0.1",
    "prettier-standard": "^8.0.1",
    "standard": "^12.0.1"
  }
}
