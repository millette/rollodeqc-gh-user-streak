{
  "name": "rollodeqc-gh-user-streak",
  "version": "4.0.0",
  "description": "Fetch github user commit streak.",
  "license": "AGPL-3.0",
  "repository": "millette/rollodeqc-gh-user-streak",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "test": "nyc ava --no-power-assert",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "lint-staged": {
    "*.{html,js,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "rollodeqc"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "got": "^9.6.0",
    "lodash.flatten": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "meow": "^5.0.0",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "coveralls": "^3.0.11",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "nyc": "^14.1.0",
    "prettier": "^1.18.2"
  }
}
